#include <iostream>
#include <cstdlib>
using namespace std;

struct Card{
	int Number;
	char Suit;
	bool Picked;
};

int getNumber(Card card){
	int x = card.Number;
	return x;
}

char getSuit(Card card){
	int x = card.Suit;
	return x;
}
